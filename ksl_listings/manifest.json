{
    "manifest_version": 3,
    "name": "KSL Average Range Pricing",
    "description": "Get average range price on ksl listings",
    "version": "1.0",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/add_average_range.js"
            ],
            "matches": [
                "https://classifieds.ksl.com/search/keyword/*"
            ],
            "run_at": "document_end"
        }
    ]
}